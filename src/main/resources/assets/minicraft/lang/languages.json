/**
  * {
      [code: string]: {  - Language unique code 
        name: string     - Name of the language
        region: string   - Region from where it originates
        locale: string   - Which locale is this based on. More useful for custom languages
      }
    }
  */
{
  "en_us": {
    "name": "English",
    "region": "US",
    "locale": "en-US"
  },
  "pt_pt": {
    "name": "Portuguese",
    "region": "Portugal",
    "locale": "pt-PT"
  },
  "pt_br": {
    "name": "Portuguese",
    "region": "Brazil",
    "locale": "pt-BR"
  }
}